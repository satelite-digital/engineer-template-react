{{#each hooks}}
{{{import}}}
{{/each}}
{{#each ui}}
{{#if component}}
import {{component}} from "../../components/{{component}}";
{{/if}}
{{/each}}

const {{name}} = () => {
  {{#each hooks}}
  {{{implementation}}}
  {{/each}}
  {{#each state}}
  {{type}} {{id}} = {{{value}}};
  {{/each}}
  {{#each actions}}
  const {{id}} = {{{value}}};
  {{/each}}
  return (
      {{#each ui}}
      <{{component}}
        {{#each props}}
        {{prop}}={ {{~value~}} }
        {{/each}}
      />
      {{/each}}
      
  );
};

export default {{name}};
