{{#if display.table}}
import Table from "../../components/Table";
{{/if}}
{{#if display.cards}}
import Card from "../../components/Card";
{{/if}}

const {{name}} = () => {
  const items = [0, 1, 2, 3, 4, 5, 6];
  return (
      {{#if display.cards}}
      <div className="grid gap-4 grid-cols-3">
        {items.map((item) => (
          <Card key={item} title="Title">
            Content {item}
          </Card>
        ))}
      </div>
      {{/if}}
      {{#if display.table}}
      <Table rows={items} />
      {{/if}}
  );
};

export default {{name}};
